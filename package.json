{
  "name": "atom-ide-datatip",
  "main": "./dist/main.js",
  "version": "0.12.2",
  "description": "A replacement for the Data Tooltip provider that was originally part of the Atom IDE package from Facebook.",
  "keywords": [
    "atom-package",
    "atom-ide",
    "datatip"
  ],
  "repository": "https://github.com/atom-ide-community/atom-ide-datatip",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix --ext js,ts,coffee,json",
    "test-debug": "npm run debug && atom --test spec",
    "test": "npm run build && atom --test spec",
    "clean": "shx rm -rf dist",
    "debug": "npm run clean && shx cp -r lib dist && env NODE_ENV=development env BABEL_ENV=development babel dist --out-dir dist",
    "dev": "npm run clean && env NODE_ENV=development env BABEL_ENV=production rollup -c -w",
    "build": "npm run clean && env NODE_ENV=production env BABEL_ENV=production rollup -c",
    "build-commit": "build-commit -o dist",
    "bump": "ncu -u",
    "prepare": "npm run build"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "atom-ide-markdown-service",
    "busy-signal"
  ],
  "dependencies": {
    "atom-package-deps": "^6.0.0",
    "dompurify": "^2.0.12",
    "etch": "^0.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/atom": "^1.40.4",
    "@types/dompurify": "^2.0.2",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "atom-languageclient": "^0.9.9",
    "jasmine": "^3.5.0",
    "@types/jasmine": "^3.5.11",
    "atom-jasmine3-test-runner": "^4.4.8",
    "typescript": "^3.9.6",
    "tslib": "^2.0.0",
    "prettier": "^2.0.5",
    "eslint": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-eslint": "latest",
    "eslint-plugin-coffee": "latest",
    "eslint-plugin-json": "latest",
    "eslint-plugin-only-warn": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "tslint": "latest",
    "tslint-config-prettier": "latest",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "commitlint": "^9.0.1",
    "rollup": "^2.19.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-commonjs": "^13.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^6.1.0",
    "shx": "^0.3.2",
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/plugin-proposal-decorators": "latest",
    "@babel/plugin-proposal-do-expressions": "latest",
    "@babel/plugin-proposal-export-default-from": "latest",
    "@babel/plugin-proposal-export-namespace-from": "latest",
    "@babel/plugin-proposal-function-bind": "latest",
    "@babel/plugin-proposal-function-sent": "latest",
    "@babel/plugin-proposal-json-strings": "latest",
    "@babel/plugin-proposal-logical-assignment-operators": "latest",
    "@babel/plugin-proposal-nullish-coalescing-operator": "latest",
    "@babel/plugin-proposal-numeric-separator": "latest",
    "@babel/plugin-proposal-optional-chaining": "latest",
    "@babel/plugin-proposal-pipeline-operator": "latest",
    "@babel/plugin-proposal-throw-expressions": "latest",
    "@babel/plugin-syntax-dynamic-import": "latest",
    "@babel/plugin-syntax-import-meta": "latest",
    "@babel/plugin-transform-async-to-generator": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-flow": "latest",
    "@babel/preset-react": "latest",
    "npm-check-updates": "^7.0.1",
    "build-commit": "^0.1.1"
  },
  "atomTestRunner": "./spec/runner",
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "providedServices": {
    "datatip": {
      "versions": {
        "0.1.0": "provideDatatipService"
      }
    }
  },
  "consumedServices": {
    "markdown-renderer": {
      "versions": {
        "1.0.0": "consumeMarkdownRenderer"
      }
    }
  }
}
